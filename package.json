{
  "name": "alb-log-analyzer",
  "version": "1.0.0",
  "type": "module",
  "description": "AWS Application Load Balancer (ALB) Log Analyzer",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "alb-log-analyzer": "./dist/main.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "download": "tsx src/scripts/download-and-analyze.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "alb",
    "log",
    "analyzer",
    "aws",
    "load-balancer"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.5",
    "@vitest/ui": "^4.0.5",
    "eslint": "^9.38.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^4.0.5"
  },
  "tsc-alias": {
    "resolveFullPaths": true,
    "replacers": {
      "exampleReplacer": {
        "enabled": false
      }
    }
  }
}
